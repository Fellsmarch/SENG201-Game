\par \pard\plain \s0\ql\widctlpar\hyphpar0\ltrpar\cf1\kerning1\dbch\af9\langfe1081\dbch\af10\afs24\alang1081\loch\f5\fs24\lang5129\ql\widctlpar\hyphpar0\ltrpar{\b\rtlch \ltrch\loch
